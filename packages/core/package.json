{
	"name": "@earlybird-sdk/core",
	"version": "0.0.0",
	"type": "module",
	"description": "Core utilities and foundational types for Earlybird SDK",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"files": ["dist"],
	"scripts": {
		"build": "tsc",
		"test": "vitest run --coverage",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage",
		"typecheck": "tsc --noEmit"
	},
	"devDependencies": {
		"@vitest/coverage-v8": "^3.1.4",
		"typescript": "^5.0.0",
		"vitest": "^3.1.4"
	},
	"keywords": ["core", "utilities", "types", "local-first"],
	"author": "Earlybird SDK Team",
	"license": "MIT"
}